{
  "name": "babel-plugin-flow-to-typescript",
  "description": "Babel plugin to convert Flow into TypeScript",
  "license": "MIT",
  "author": {
    "name": "Yuichiro Kikura",
    "email": "y.kikura@gmail.com",
    "url": "https://github.com/Kiikurage"
  },
  "contributors": [
    {
      "name": "CureApp"
    }
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "version": "0.4.0",
  "scripts": {
    "prepare": "npm run build",
    "test": "jest",
    "test-watch": "jest --watch",
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "tslint --project .",
    "cover": "jest --coverage",
    "prettify": "./node_modules/.bin/prettier --write \"{src,test}/**/*.{js,ts,tsx}\""
  },
  "keywords": [
    "babel",
    "flow",
    "typescript"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kiikurage/babel-plugin-flow-to-typescript.git"
  },
  "peerDependencies": {
    "@babel/core": "^7.2.2"
  },
  "dependencies": {
    "@babel/traverse": "^7.2.3",
    "@babel/types": "^7.2.2"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.0.7",
    "babel-plugin-tester": "^6.4.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
