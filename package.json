{
  "name": "@steelbrain/babel-plugin-flow-to-typescript",
  "description": "Babel plugin to convert Flow into TypeScript",
  "license": "MIT",
  "author": {
    "name": "Yuichiro Kikura",
    "email": "y.kikura@gmail.com",
    "url": "https://github.com/Kiikurage"
  },
  "files": [
    "dist/*",
    "README.md"
  ],
  "version": "0.5.0",
  "scripts": {
    "prepare": "npm run build",
    "test": "jest",
    "test-watch": "jest --watch",
    "build": "tsc",
    "watch": "tsc --watch",
    "format:check": "prettier --list-different {src,test}/**/*.ts"
  },
  "keywords": [
    "babel",
    "flow",
    "typescript"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kiikurage/babel-plugin-flow-to-typescript.git"
  },
  "peerDependencies": {
    "@babel/core": "^7.4.4"
  },
  "dependencies": {
    "@babel/traverse": "^7.4.4",
    "@babel/types": "^7.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@types/babel-core": "^6.25.6",
    "@types/babel-types": "^7.0.7",
    "@types/node": "^10.14.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-tester": "^5.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.17.1",
    "regenerator-runtime": "^0.13.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "jest": {
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
